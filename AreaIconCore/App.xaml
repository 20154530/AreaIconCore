<Application x:Class="AreaIconCore.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AreaIconCore">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/YControls;component/Themes/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <FontFamily x:Key="Questrial">
                pack://application:,,,/FontLibrary;component/Questrial.ttf#Questrial
            </FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
