<Page x:Class="AreaIconCore.Views.Pages.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:yt_button ="clr-namespace:YControls.FontIconButtons;assembly=YControls"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:AreaIconCore.Views"
      Height="360">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <yt_button:FIconButton Grid.Row="1" 
                               Style="{StaticResource SettingPageSaveButton}"
                               Click="FIconButton_Click"/>
    </Grid>
</Page>
